<?xml version="1.0" encoding="UTF-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:drawable="@drawable/ic_seekbar_white" />
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient
                    android:startColor="#f5f59e07"
                    android:endColor="#f5f59e07"
                    android:angle="270.0"
                    android:centerY="0.75"
                    android:centerColor="#a1f9eac6" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip android:drawable="@drawable/ic_seekbar_orange" />
    </item>
</layer-list>